<%include partials/header %>

<div class="container">
<header class ="jumbotron">
    <h1>Welcome to Corbital Pvt Ltd</h1>
    <a class="btn btn-primary btn-large" href="/campgrounds/new">Add new Sheet</a>
    
    
</header>


<div clas="row text center" style ="display:flex; flex-wrap:wrap;">  
   <%campgrounds.forEach(function(campground){ %>
<div class="col-md-3 col-sm-6">
    <div class="thumbnail">
           <img src="<%=campground.image%>">
           <div class="caption">
               <h4> <%=campground.name %> </h4>
           </div>
           </div>
     
</div>
<% }); %> 

</div>
</div>

